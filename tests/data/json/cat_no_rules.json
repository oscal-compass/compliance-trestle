    "groups": [
      {
        "id": "AC.2",
        "title": "ACCOUNT MANAGEMENT",
        "props": [
          {
            "name": "label",
            "value": "AC.2"
          }
        ],
        "controls": [
          {
            "id": "AC.2.1.10",
            "class": "EC",
            "title": "AUTOMATED SYSTEM ACCOUNT MANAGEMENT",
            "props": [
              {
                "name": "label",
                "value": "AC.2.1.10"
              }
            ],
            "parts": [
              {
                "id": "AC.2.1.10_smt",
                "name": "statement",
                "prose": "The organization employs automated mechanisms to support the management of information system accounts"
              },
              {
                "id": "AC.2.1.10_gdn",
                "name": "guidance",
                "props": [
                  {
                    "name": "label",
                    "value": "2."
                  },
                  {
                    "name": "alt-identifier",
                    "value": "Requirements"
                  }
                ],
                "prose": "The CSP shall document the scope of the cloud service that is under the CSPâ€™s control and the boundaries."
              }
            ]
          },
          {
            "id": "AC.2.1.11",
            "class": "EC",
            "title": "REMOVAL OF TEMPORARY ACCOUNTS",
            "props": [
              {
                "name": "label",
                "value": "AC.2.1.11"
              }
            ],
            "parts": [
              {
                "id": "AC.2.1.11_smt",
                "name": "statement",
                "prose": "The information system automatically  disables temporary accounts after 1 day."
              },
              {
                "id": "AC.2.1.11_gdn",
                "name": "guidance",
                "props": [
                  {
                    "name": "label",
                    "value": "2."
                  },
                  {
                    "name": "alt-identifier",
                    "value": "Requirements"
                  }
                ],
                "prose": "This control enhancement requires the removal of both temporary and emergency accounts automatically after a predefined period of time has elapsed, rather than at the convenience of the systems administrator."
              }
            ]
          }
        ]
      },
      {
        "id": "AC.3",
        "title": "ACCESS ENFORCEMENT",
        "props": [
          {
            "name": "label",
            "value": "AC.3"
          }
        ],
        "controls": [
          {
            "id": "AC.3.1.10",
            "class": "EC",
            "title": "DUAL AUTHORIZATION",
            "props": [
              {
                "name": "label",
                "value": "AC.3.1.10"
              }
            ],
            "parts": [
              {
                "id": "AC.3.1.10_smt",
                "name": "statement",
                "prose": "The information system enforces dual authorization for organization-defined privileged commands"
              },
              {
                "id": "AC.3.1.10_gdn",
                "name": "guidance",
                "props": [
                  {
                    "name": "label",
                    "value": "2."
                  },
                  {
                    "name": "alt-identifier",
                    "value": "Requirements"
                  }
                ],
                "prose": "Dual authorization mechanisms require the approval of two authorized individuals in order to execute. Organizations do not require dual authorization mechanisms when immediate responses are necessary to ensure public and environmental safety. Dual authorization may also be known as two-person control."
              }
            ]
          },
          {
            "id": "AC.3.1.11",
            "class": "EC",
            "title": "MANDATORY ACCESS ENFORCEMENT",
            "props": [
              {
                "name": "label",
                "value": "AC.3.1.11"
              }
            ],
            "parts": [
              {
                "id": "AC.3.1.11_smt",
                "name": "statement",
                "prose": "The information system enforces  mandatory access control policy] over all subjects and objects where the policy is uniformly enforced across all subjects and objects within the boundary of the information system"
              },
              {
                "id": "AC.3.1.11_gdn",
                "name": "guidance",
                "props": [
                  {
                    "name": "label",
                    "value": "2."
                  },
                  {
                    "name": "alt-identifier",
                    "value": "Requirements"
                  }
                ],
                "prose": "Mandatory access control as defined in this control enhancement is synonymous with nondiscretionary access control, and is not constrained only to certain historical uses."
              }
            ]
          }
        ]
      },
      {
        "id": "CM.8",
        "title": "INFORMATION SYSTEM COMPONENT INVENTORY ",
        "props": [
          {
            "name": "label",
            "value": "CM.8"
          }
        ],
        "controls": [
          {
            "id": "CM.8.2.2",
            "class": "EC",
            "title": "AUTOMATED MAINTENANCE",
            "props": [
              {
                "name": "label",
                "value": "CM.8.2.2"
              }
            ],
            "parts": [
              {
                "id": "CM.8.2.2_smt",
                "name": "statement",
                "prose": "The organization employs automated mechanisms to help maintain an up-to-date, complete, accurate, and readily available inventory of information system components."
              },
              {
                "id": "CM.8.2.2_gdn",
                "name": "guidance",
                "props": [
                  {
                    "name": "label",
                    "value": "2."
                  },
                  {
                    "name": "alt-identifier",
                    "value": "Requirements"
                  }
                ],
                "prose": "Organizations maintain information system inventories to the extent feasible."
              }
            ]
          }
        ]
      },
      {
        "id": "CP.9",
        "title": "INFORMATION SYSTEM BACKUP",
        "props": [
          {
            "name": "label",
            "value": "CP.9"
          }
        ],
        "controls": [
          {
            "id": "CP.9.1",
            "class": "EC",
            "title": "TESTING FOR RELIABILITY",
            "props": [
              {
                "name": "label",
                "value": "CP.9.1"
              }
            ],
            "parts": [
              {
                "id": "CP.9.1_smt",
                "name": "statement",
                "prose": "The organization tests backup information every 3 months to verify media reliability and information integrity."
              },
              {
                "id": "CP.9.1_gdn",
                "name": "guidance",
                "props": [
                  {
                    "name": "label",
                    "value": "2."
                  },
                  {
                    "name": "alt-identifier",
                    "value": "Requirements"
                  }
                ],
                "prose": "Critical information system software includes, for example, operating systems, cryptographic key management systems, and intrusion detection/prevention systems"
              }
            ]
          },
          {
            "id": "CP.9.2",
            "class": "EC",
            "title": "TEST RESTORATION USING SAMPLING",
            "props": [
              {
                "name": "label",
                "value": "CP.9.2"
              }
            ],
            "parts": [
              {
                "id": "CP.9.2_smt",
                "name": "statement",
                "prose": "The organization uses a sample of backup information in the restoration of selected information system functions as part of contingency plan testing"
              },
              {
                "id": "CP.9.2_gdn",
                "name": "guidance",
                "props": [
                  {
                    "name": "label",
                    "value": "2."
                  },
                  {
                    "name": "alt-identifier",
                    "value": "Requirements"
                  }
                ],
                "prose": "Information system backup information can be transferred to alternate storage sites either electronically or by physical shipment of storage media"
              }
            ]
          }
        ]
      },
      {
        "id": "IA.5",
        "title": "AUTHENTICATOR MANAGEMENT ",
        "props": [
          {
            "name": "label",
            "value": "IA.5"
          }
        ],
        "controls": [
          {
            "id": "IA.5.1.4",
            "class": "EC",
            "title": "AUTOMATED SUPPORT FOR PASSWORD STRENGTH DETERMINATION",
            "props": [
              {
                "name": "label",
                "value": "IA.5.1.4"
              }
            ],
            "parts": [
              {
                "id": "IA.5.1.4_smt",
                "name": "statement",
                "prose": "The organization employs automated tools to determine if password authenticators are sufficiently strong to satisfy requirements."
              },
              {
                "id": "IA.5.1.4_gdn",
                "name": "guidance",
                "props": [
                  {
                    "name": "label",
                    "value": "2."
                  },
                  {
                    "name": "alt-identifier",
                    "value": "Requirements"
                  }
                ],
                "prose": "This control enhancement focuses on the creation of strong passwords and the characteristics of such passwords."
              }
            ]
          }
        ]
      }
    ],
